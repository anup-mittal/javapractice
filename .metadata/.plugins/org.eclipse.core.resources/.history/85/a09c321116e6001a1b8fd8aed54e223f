package amanpackage;

import java.util.Scanner;

public class AmanMain {
	public static void main (String[] args)
	{
		int rows,columns;
		Scanner sc = new Scanner(System.in);
		System.out.println("enter rows in square: ");
		rows=sc.nextInt();
		System.out.println("enter columns in square: ");
		columns=sc.nextInt();
		
		Patterns p=new Patterns();
		p.square(rows,columns);
		Patterns a=new Patterns();
		a.triangle();
	}

}
