package amanpackage;

public class Patterns {
	public void square(int i,int j)
	{
		for(i=0;i<=4;i++)
		{
			for(j=0;j<=4;j++)
			{
			    System.out.println("*");
			}
			System.out.println("\n");
		}
	}
	public void triangle(int i,int j)
	{
		for(i=0;i<=4;i++)
		{
			for(j=0;j<=4;j++)
			{
				System.out.println("*");
			}
			System.out.println("\n");
		}
	}

}
